  "version": "0.1.0",
  "description": "TypeScript SDK for PayBot â€” Let your AI agent pay for things",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
