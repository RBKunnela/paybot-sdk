{
  "name": "paybot-sdk",
  "version": "0.1.0",
  "description": "PayBot SDK - USDC payments for bots via x402 protocol",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "x402",
    "payments",
    "usdc",
    "bot",
    "ai-agent",
    "eip-3009",
    "base",
    "paybot",
    "mcp",
    "bot-payments"
  ],
  "author": "RBKunnela",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RBKunnela/paybot-sdk.git"
  },
  "homepage": "https://paybotcore.com",
  "bugs": {
    "url": "https://github.com/RBKunnela/paybot-sdk/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "viem": "^2.46.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  }
}
